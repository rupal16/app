@import (reference) './../../variables.less';

@fontName          : 'Source Sans Pro';
@headerFont        : 'Dosis', sans-serif;
@pageFont          : 'Open Sans', sans-serif;
@importGoogleFonts : false;

@emSize   : 14px;
@fontSize : 14px;

@primaryColor: @primary;
@secondaryColor: @accent;
@lightPrimaryColor: lighten(@primary, 10%);
@lightSecondaryColor: lighten(@accent, 10%);


@textColor: rgb(99,99,99);
@linkColor: @primary;
